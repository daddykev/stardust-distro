# template/.env.example
# Firebase Configuration
VITE_FIREBASE_API_KEY=your_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your_project_id.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_project_id.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your_sender_id_here
VITE_FIREBASE_APP_ID=your_app_id_here
VITE_FIREBASE_MEASUREMENT_ID=your_measurement_id_here

# Application Configuration
VITE_APP_NAME=My Music Distro
VITE_ORGANIZATION_ID=my-music-distro
VITE_DISTRIBUTOR_ID=my-distributor-id

# Cloud Functions URL (auto-generated in most cases)
VITE_FUNCTIONS_URL=https://us-central1-your_project_id.cloudfunctions.net

# GitHub/NPM (for splash page - optional)
VITE_GITHUB_REPO=https://github.com/yourusername/yourrepo
VITE_NPM_PACKAGE=@yourscope/yourpackage